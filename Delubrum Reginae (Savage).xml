  <s name="ダウー">

    <!-- マーカー付与時トリガ start -->
    <t no="1" name="InsertDiceMarker" sync="^1B:[id8]:[pc]:[id8]:(?&lt;mark_code&gt;[0-9a-fA-F]{4}):" sync-count="1-8" >
      <expressions>
        <table>
        {
            "method":"Insert",
            "table":"DiceTable",
            "cols" : [
              {
                "name" : "marker",
                "val" : "${mark_code}",
                "key" : "true"
              },
              {
                "name":"target",
                "val":"${_pc}"
              },
            ]
        }
        </table>
      </expressions>
    </t>

    <t no="2" text="マーカー1番" sync="^1B:" sync-count="4" notice="いちばん、北えー。" >
      <expressions>
        <pre name="TABLE['DiceTable'][0]['target']" value="@Model.Player.Name" />
        <set name="call_delay" value="1" />
      </expressions>
      <v-notice
        text="マーカー1番 北"
        duration="10"
        icon="Targetaoe03.png"
        duration-visible="false" />
    </t>

    <t no="3" text="マーカー2番" sync="^1B:" sync-count="4" notice="にばん、東えー。" >
